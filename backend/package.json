{
  "name": "tmurt-backend",
  "version": "1.0.0",
  "description": "Tmurt - Moroccan Traditional Clothing Store",
  "author": "Mouad El Aroui",
  "license": "MIT",
  "keywords": [
    "medusa",
    "ecommerce",
    "morocco",
    "traditional",
    "clothing"
  ],
  "scripts": {
    "build": "medusa build",
    "start": "medusa start",
    "start:production": "npx medusa db:migrate && npx medusa build && medusa start",
    "dev": "medusa develop",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "seed:store": "medusa exec ./src/scripts/seed-store.ts",
    "seed:products": "medusa exec ./src/scripts/seed-products.ts",
    "seed:modules": "medusa exec ./src/scripts/seed-modules.ts",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.12.3",
    "@medusajs/cli": "2.12.3",
    "@medusajs/framework": "2.12.3",
    "@medusajs/medusa": "2.12.3",
    "@medusajs/ui": "^4.0.31"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.12.3",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.14",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}